<% provide(:title, 'Sign IN nao') %>
<h1>sign in here bub</h1>

<div class="row">
	<div class="span6 offset3">
		
		<%= form_for(:session, url: sessions_path) do |f| %>

			<%= f.label :email %>
			<%= f.text_field :email %>

			<%= f.label :password %>
			<%= f.password_field :password %>

			<%= f.submit "sign en", class: "btn btn-large btn-primary" %>
		<% end %>

		<p>Are you new?  If so, click <%= link_to("here", register_path) %> pls~</p>

	</div>
</div>